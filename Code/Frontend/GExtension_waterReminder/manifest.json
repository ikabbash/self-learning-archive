{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Water Reminder",
    "description": "Dummy water reminder extension",
    "icons": {
        "128": "./icons/128.png",
        "48": "./icons/48.png",
        "32": "./icons/32.png",
        "16": "./icons/16.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "background":{
        "service_worker": "background.js"
    },
    "permissions":[
        "alarms",
        "notifications"
    ]
}